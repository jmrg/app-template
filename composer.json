{
    "name": "jmrg/app-template",
    "description": "App template with PHP Micro-Framework based on Chip-Modules.",
    "keywords": ["app", "app-template", "micro-framework", "framework", "chip-modules", "fast", "php", "components"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "jmrg/chip-modules": "~1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
